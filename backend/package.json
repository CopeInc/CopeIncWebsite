{
  "name": "griefing-website-backend",
  "version": "1.0.0",
  "description": "Backend API for Minecraft griefing group website",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5",
    "express-rate-limit": "^6.7.0",
    "joi": "^17.7.0",
    "dotenv": "^16.0.0",
    "@prisma/client": "^4.10.0",
    "prisma": "^4.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/multer": "^1.4.0",
    "@types/node": "^18.14.0",
    "@types/jest": "^29.4.0",
    "@types/supertest": "^2.0.0",
    "typescript": "^4.9.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.4.0",
    "ts-jest": "^29.0.0",
    "supertest": "^6.3.0"
  }
}